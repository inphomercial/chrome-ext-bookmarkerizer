{
  "name": "Free Your Bookmarks",
  "version": "1.0",
  "description": "View your bookmarks by added date in a very simple list, easier to identify and remove unneeded bookmarks.",
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
      "default_title": "Free Your Bookmarks",
      "default_icon": "icon48.png"
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
